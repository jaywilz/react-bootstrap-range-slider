    "cross-env": "^7.0.0",
    "jest": "^25.1.0",
    "less": "^3.10.3",
    "react": "^16.12.0",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-dom": "^16.12.0",
    "react-test-renderer": "^16.12.0",
    "rollup": "^1.29.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-less": "^1.1.2",
    "rollup-plugin-license": "^0.13.0",
    "rollup-plugin-replace": "^2.2.0"
  },
  "dependencies": {
